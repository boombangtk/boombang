	function popUp(URL)
	{
	  var options = "";
	  var direccion= URL;
	  {
	    var w = screen.width;
	    var h = screen.height;
	    var winw = 1012;
	    var winh = 657;
	    var winl = (w - winw)/2;
	    var wint = (h -winh)/2;
	    options="toolbar=0,scrollbars=0,location=0,statusbar=0,menubar=0,resizable=0,width="+winw+",height="+winh+",left="+winl+",top ="+wint;
	   }
	   var win = window.open(direccion, 'BoomBang', options);
	}
</script>

